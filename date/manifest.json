{
  "manifest_version": 2,
  "name": "Start Page - Date and Time",
  "description": "Displays bing.com's daily wallpaper on new tabs as well as date and time and weather widget.",
  "version": "0.1.6",
  "background": {
    "persistent": false,
    "scripts" : [
      "background.js"
    ]
  },
  "chrome_url_overrides": {
    "newtab": "data/override/index.html"
  },
  "permissions": [
    "storage",
    "chrome://favicon/",
    "bookmarks",
    "tabs",
    "topSites",
    "contextMenus",
    "*://api.openweathermap.org/*",
    "*://www.bing.com//HPImageArchive.aspx?*",
    "*://www.google.com/s2/*"
  ],
  "homepage_url": "https://add0n.com/new-tab.html?from=date",
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "64": "data/icons/64.png",
    "128": "data/icons/128.png",
    "256": "data/icons/256.png",
    "512": "data/icons/512.png"
  }
}
